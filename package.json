{
  "name": "contribux",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:db": "vitest run --config vitest.database.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:ci": "vitest run --config vitest.integration.config.ts --reporter=verbose --bail=1",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ci": "playwright test --reporter=line",
    "test:unit": "vitest run --run",
    "test:unit:enhanced": "vitest run --config vitest.config.ts",
    "test:security": "vitest run --config vitest.config.ts --grep security",
    "test:db:enhanced": "vitest run --config vitest.database.config.ts",
    "test:integration:enhanced": "vitest run --config vitest.integration.config.ts",
    "test:all:enhanced": "pnpm test:unit:enhanced && pnpm test:db:enhanced && pnpm test:integration:enhanced",
    "playwright:install": "playwright install --with-deps",
    "db:setup": "bash scripts/setup-local-db.sh",
    "db:test-connection": "node scripts/test-local-db.js",
    "db:migrate": "node scripts/db-migrations/run-migrations-enhanced.cjs run",
    "db:migrate:test": "NODE_ENV=test node scripts/db-migrations/run-migrations-enhanced.cjs run",
    "db:migrate:status": "node scripts/db-migrations/run-migrations-enhanced.cjs status",
    "db:migrate:reset": "node scripts/db-migrations/run-migrations-enhanced.cjs reset",
    "db:health": "node -e \"console.log('Database health check...')\"",
    "db:performance-report": "node -e \"console.log('Performance report...')\"",
    "db:vector-metrics": "node -e \"console.log('Vector metrics...')\"",
    "perf:analyze": "node scripts/performance-analysis.js",
    "perf:bundle": "node scripts/bundle-analyzer.js",
    "perf:report": "npm run perf:analyze && npm run perf:bundle",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "perf:dashboard": "echo 'Visit /performance in your app to view the performance dashboard'",
    "observability:up": "docker-compose -f docker-compose.observability.yml up -d",
    "observability:down": "docker-compose -f docker-compose.observability.yml down",
    "observability:logs": "docker-compose -f docker-compose.observability.yml logs -f",
    "observability:status": "docker-compose -f docker-compose.observability.yml ps",
    "health:check": "curl -s http://localhost:3000/api/health | jq",
    "metrics:view": "curl -s http://localhost:3000/api/metrics"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@octokit/plugin-retry": "^8.0.1",
    "@octokit/plugin-throttling": "^11.0.1",
    "@octokit/rest": "^22.0.0",
    "@octokit/types": "^14.1.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.62.0",
    "@opentelemetry/exporter-jaeger": "^2.0.1",
    "@opentelemetry/exporter-metrics-otlp-http": "0.203.0",
    "@opentelemetry/exporter-prometheus": "0.203.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.203.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.203.0",
    "@opentelemetry/instrumentation": "0.203.0",
    "@opentelemetry/instrumentation-express": "0.52.0",
    "@opentelemetry/instrumentation-fs": "0.23.0",
    "@opentelemetry/instrumentation-http": "0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "0.203.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@redis/client": "^5.5.6",
    "@simplewebauthn/server": "^13.1.2",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.82.0",
    "@tanstack/react-query-devtools": "^5.82.0",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "drizzle-orm": "^0.44.2",
    "framer-motion": "^12.23.3",
    "ioredis": "^5.6.1",
    "ipaddr.js": "^2.2.0",
    "jose": "^6.0.11",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "pg": "^8.13.1",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.5",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^7.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^3.0.2",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5.8.3",
    "web-vitals": "^5.0.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@electric-sql/pglite": "^0.3.4",
    "@faker-js/faker": "^9.9.0",
    "@next/bundle-analyzer": "^15.3.5",
    "@playwright/test": "1.54.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.13",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^3.2.0",
    "@vitest/ui": "^3.2.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.30.1",
    "eslint-config-next": "^15.3.4",
    "glob": "^11.0.3",
    "jsdom": "^26.0.0",
    "msw": "^2.6.0",
    "next-test-api-route-handler": "^4.0.16",
    "postcss": "^8.5.6",
    "puppeteer": "^24.12.1",
    "supertest": "^7.1.3",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "undici": "^7.11.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^3.2.0"
  }
}